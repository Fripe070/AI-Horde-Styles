{
    "problemMatcher": [
        {
            "owner": "styles",
            "pattern": [
                {
                    "regexp": "Validating [0-9]+ styles from (.+)\\.json",
                    "file": 1
                },
                {
                    "regexp": "validation errors? for .+?\n(.+) \\[type=",
                    "message": 1
                }
            ]
        }
    ]
}